{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(ask|health|progress|check_answer|generate_title|memory)(.*)",
      "dest": "api/index.py"
    }
  ]
}
